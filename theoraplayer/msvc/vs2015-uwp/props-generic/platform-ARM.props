      <PreprocessorDefinitions>_WINUWP;WINUWP;_WINRT;WINRT;_WINARM;WINARM;_ARM;ARM;__ARM__;__arm__;_CRT_SECURE_NO_WARNINGS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
