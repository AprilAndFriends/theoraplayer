<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="Configuration">
    <CharacterSet>Unicode</CharacterSet>
    <PlatformToolset>v120_wp81</PlatformToolset>
    <WindowsAppContainer>true</WindowsAppContainer>
  </PropertyGroup>
  <Import Condition="(exists('..\..\lib\msvc\vs2013-winp8\$(Platform).props'))" Project="..\..\lib\msvc\vs2013-winp8\$(Platform).props"/>
  <Import Condition="(exists('..\..\msvc\vs2013-winp8\$(Platform).props'))" Project="..\..\msvc\vs2013-winp8\$(Platform).props"/>
  <Import Condition="(exists('..\..\..\msvc\vs2013-winp8\$(Platform).props'))" Project="..\..\..\msvc\vs2013-winp8\$(Platform).props"/>
  <Import Condition="(exists('..\..\..\..\msvc\vs2013-winp8\$(Platform).props'))" Project="..\..\..\..\msvc\vs2013-winp8\$(Platform).props"/>
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>_WINRT;WINRT;_WINP8;WINP8;_WIN32;WIN32;_X86;X86;_CRT_SECURE_NO_WARNINGS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>../..;../../src;../../../include;../../../../include;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <StringPooling>true</StringPooling>
      <CompileAsWinRT>true</CompileAsWinRT>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories Condition="'$(WholeProgramOptimization)'!='true'">$(SolutionDir)bin\Debug\$(Platform)-WinP8;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalLibraryDirectories Condition="'$(WholeProgramOptimization)'=='true'">$(SolutionDir)bin\Release\$(Platform)-WinP8;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalLibraryDirectories>..\..\lib\lib\msvc120-winp8\$(Platform);..\..\lib\msvc120-winp8\$(Platform);..\..\..\lib\msvc120-winp8\$(Platform);..\..\..\..\lib\msvc120-winp8\$(Platform);%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <TargetMachine>MachineX86</TargetMachine>
      <PrecompiledHeader>NotUsing</PrecompiledHeader>
    </Link>
    <Lib>
      <AdditionalLibraryDirectories Condition="'$(WholeProgramOptimization)'!='true'">$(SolutionDir)bin\Debug\$(Platform)-WinP8;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalLibraryDirectories Condition="'$(WholeProgramOptimization)'=='true'">$(SolutionDir)bin\Release\$(Platform)-WinP8;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalLibraryDirectories>..\..\lib\lib\msvc120-winp8\$(Platform);..\..\lib\msvc120-winp8\$(Platform);..\..\..\lib\msvc120-winp8\$(Platform);..\..\..\..\lib\msvc120-winp8\$(Platform);%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <TargetMachine>MachineX86</TargetMachine>
      <PrecompiledHeader>NotUsing</PrecompiledHeader>
    </Lib>
  </ItemDefinitionGroup>
</Project>